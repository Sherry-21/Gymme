{
  "expo": {
    "name": "GymMe",
    "slug": "GymMe",
    "version": "1.0.0",
    "icon": "./assets/images/icon.png",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.devinluize.GymMe"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#fff",
          "image": "./assets/images/splash.png",
          "dark": {
            "image": "./assets/images/splash.png",
            "backgroundColor": "#fff"
          },
          "resizeMode": "cover"
        }
      ],
      "expo-video",
      "expo-secure-store",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "packagerOpts": {
      "sourceExts": [
        "js",
        "jsx",
        "svgx"
      ],
      "transformer": "node_modules/react-native-svg-transformer/index.js"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ad0d3501-0a86-48e7-94b7-5b3463244a10"
      }
    }
  }
}
